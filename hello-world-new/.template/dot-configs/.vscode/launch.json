{
 
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "smartStep": true,
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}/src",
            "program": "index.ts",
            "env": {
                "GS_TRANSPILE": "true"
            },
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}