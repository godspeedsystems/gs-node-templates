{
	"name": "gs-node-starter-kit",
	"version": "1.0.0",
	"description": "My first godspeed project.",
	"main": "dist/index.js",
	"scripts": {
		"dev": "nodemon --watch src -e ts,js,yaml,json -r dotenv ./src/index.ts",
		"gen-crud-api": "npx @godspeedsystems/api-generator",
		"build": "swc src -d dist && rsync -a --exclude '*.ts' ./src/ ./dist/ && rsync -a ./config/ ./dist/config && rsync -a ./src/mappings/ ./dist/mappings && rsync -a ./src/plugins/ ./dist/plugins",
		"preview": "node dist/index.js",
		"clean": "rm -rf dist",
		"serve": "nodemon --watch src --watch config --watch mappings --watch plugins -e ts,js,yaml,json,.env --exec 'npm run clean && npm run build && npm run preview'"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.93",
		"@types/config": "^3.3.0",
		"@types/node": "^18.14.2",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@apollo/server": "^4.10.0",
		"@aws-sdk/client-dynamodb": "^3.518.0",
		"@aws-sdk/client-s3": "^3.515.0",
		"@godspeedsystems/api-generator": "^2.1.0",
		"@godspeedsystems/core": "^2.4.3",
		"@godspeedsystems/plugins-aws-as-datasource": "^1.1.1",
		"@godspeedsystems/plugins-axios-as-datasource": "^1.0.1",
		"@godspeedsystems/plugins-express-as-http": "^1.0.16",
		"@godspeedsystems/plugins-graphql-as-eventsource": "^1.0.1",
		"@godspeedsystems/plugins-kafka-as-datasource-as-eventsource": "^1.0.3",
		"@godspeedsystems/plugins-prisma-as-datastore": "^1.0.6",
		"@godspeedsystems/plugins-salesforce-as-datasource-as-eventsource": "^1.0.0",
		"@godspeedsystems/test": "^0.0.1",
		"@prisma/client": "^5.2.0",
		"config": "^3.3.9",
		"dotenv": "^16.0.3",
		"fs-extra": "^11.2.0",
		"graphql": "^16.8.1",
		"pino-pretty": "^10.2.0",
		"prisma": "^5.2.0"
	}
}
